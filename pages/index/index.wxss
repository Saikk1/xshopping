/**index.wxss**/
/* 轮播图样式 */


.wx-swiper-dots{  
  left: auto;
  right: 0;
  bottom: 0; 
}
.wx-swiper-dot{
  width: 20rpx;height: 6rpx;
  background-color: #000000;
  border-radius: 6rpx;
  opacity: 0.2;
  margin:0 4rpx; 
}
.wx-swiper-dot-active{
  opacity: 1;
}

.textview{
  display: flex;
  justify-content: center;
}

/* 输入框样式 */
.input{
  height: 35px;
  border:white solid 15rpx;
  border-radius: 40px;
  font-size: 16px;
  z-index: 100;
  display:flex;
  flex-grow: 1;
  position: relative;
  margin:30rpx;
  background-color: white;
}

.searchicon{
  height: 50rpx;
  width: 50rpx;
  opacity: 0.5;
  margin-left: 10px; 
}

.searchBar{
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 50px;
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-size: cover;
  background-position: bottom;
}

.barview{
  float: left;
}
.barview-r{
  margin-left: 350rpx;
  margin-top: 0rpx;
}

.txt{
  color:rgb(138, 127, 127);
  font-size: 20rpx;
}

.pritxt{
  color:#d81e06;
  font-weight:bold;
  margin-left: 25rpx;
}

.vview{
  height:15rpx;
}

.nx_swiper swiper{
  width: 650rpx;
  height: 400rpx;
}

.nx_swiper swiper image{
  width: 400rpx;
  height: 400rpx;
}



.navigate-item {
  display: inline-block;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  margin: 0 16rpx;
}

.names {
  font-size: 28rpx;
  color: #3c3c3c;
}

.names.active {
  color: #d81e06;
  font-weight: bold;
  font-size: 34rpx;
}

.currtline {
  margin: -8rpx auto 0 auto;
  width: 100rpx;
  height: 8rpx;
  border-radius: 4rpx;
}

.currtline.active {
  background: #d81e06;
  transition: all .3s;
}

.sswiper {
  min-height: calc(100vh - 200rpx);
}
.sswiper-item {
  width: 100%;
  text-align: center;
  padding: 32rpx 0;
}


.rankNum{
  font-size: 35rpx;
  font-weight: bold;
  display: flex;
  float: left;
}

.rateNum{
  width:120rpx;
  height:120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  line-height:35rpx;
  font-weight: bold;
  color: white;
  font-size: 50rpx;
  background-color: #6c3;
  border-radius: 40rpx;
}

/*宫格列表*/
.nx_congar{
  margin:20 rpx;
}

.nx_congar_title{
  padding: 20rpx;
  text-align: left;
  color:white;
  font-weight:bold;
  font-size: 40rpx;
  background-color: #d81e06;
  border-bottom: 10rpx solid rgb(190, 186, 186);
  border-radius: 10rpx 10rpx 0 0;
  letter-spacing: 1rpx; 
  background-image: url("data:image/png;base64,");
  background-repeat:no-repeat;
  background-size: cover;
  background-position: bottom;
}

.nx_congar_list{
  padding: 20rpx 20rpx 0 20rpx;
  display:flex;
  flex-direction: row;
  flex-wrap: wrap;
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
}

.nx_congar_list navigator{
  margin-bottom: 20rpx;
  text-align: left;
  color:black;
  border-bottom: 1px solid rgb(223, 223, 223);
  border-radius: 5rpx;
}

.nx_congar_list1{
  padding: 20rpx 20rpx 0 20rpx;
  display:flex;
  flex-direction: row;
  flex-wrap: wrap;
  background-color: white;
  border-radius: 0 0 10rpx 10rpx;
}
.nx_congar_list1 navigator{
  margin-bottom: 20rpx;
  text-align: center;
  color:black;
  border-bottom: 1px solid rgb(223, 223, 223);
  border-radius: 5rpx;
}


.nx_congar_item_1{
  width:100%;
}

.nx_congar_item_1 image{
  height:180rpx;
  width: fit-content;
}

.nx_congar_item_3{
  width:33.3%;
  
}

.item3{
  display:inline-block;
}

.item3Border{
  border-bottom: #d81e06 0rpx solid;
  border-left: #d81e06 0rpx solid;
  border-right: #d81e06 0rpx solid;
  border-top:#d81e06 0rpx solid;
  background-color: white;
}

.nx_congar_item_3 image{
  width:90%*33.3%;
  height:270rpx;
}

.nx_congar_item_4{
  width:25%;
}

.item4{
  display:inline-block;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.nx_congar_item_4 image{
  width:90%*33.3%;
  height:125rpx;
}

.nx_item_name{
  display: -webkit-box;
  overflow: hidden;
  margin-left: 25rpx;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}


